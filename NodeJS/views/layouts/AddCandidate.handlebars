<section class="container-fluid">
    <h1>Add a candidate</h1>
    <div id="elementHello"></div>
    <script>
        $('#frmAddCandidate').attr('onsubmit', 'return false;');
        $(document).on("submit", "form", function (e) {
            e.preventDefault();
            alert('it works!');
            return false;
        });
    </script>
    <form id="frmAddCandidate">
        <div class="table-responsive col-6">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Age</th>
                        <th>Eligible to run?</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <input type="text" id="txtFName" placeholder="First" />
                        </td>
                        <td>
                            <input type="text" id="txtLName" placeholder="Last" />
                        </td>
                        <td>
                            <input type="text" id="txtLAge" placeholder="Age" />
                        </td>
                        <td>
                            <input type="checkbox" id="chkEligible" placeholder="Last" />
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <input type="submit" class="btn btn-info" value="Submit Button" />
    </form>
    <p id="subResult"></p>
</section>